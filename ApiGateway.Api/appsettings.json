{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "auth_route": {
        "ClusterId": "auth_cluster",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        }
      },
      "customers_route": {
        "ClusterId": "customers_cluster",
        "Match": {
          "Path": "/api/customers/{**catch-all}"
        }
      },
      "inventories_route": {
        "ClusterId": "inventories_cluster",
        "Match": {
          "Path": "/inventories/{**catch-all}"
        }
      },
      "orders_route": {
        "ClusterId": "orders_cluster",
        "Match": {
          "Path": "/api/orders/{**catch-all}"
        }
      },
      "payments_route": {
        "ClusterId": "payments_cluster",
        "Match": {
          "Path": "/api/payments/{**catch-all}"
        }
      },
      "products_route": {
        "ClusterId": "products_cluster",
        "Match": {
          "Path": "/api/products/{**catch-all}"
        }
      },
      "reports_route": {
        "ClusterId": "reports_cluster",
        "Match": {
          "Path": "/api/reports/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth_cluster": {
        "Destinations": {
          "auth_destination": {
            "Address": "https://localhost:8000/"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        }
      },
      "customers_cluster": {
        "Destinations": {
          "customers_destination": {
            "Address": "https://localhost:8001/"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        }
      },
      "inventories_cluster": {
        "Destinations": {
          "inventories_destination": {
            "Address": "https://localhost:8002/"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        }
      },
      "orders_cluster": {
        "Destinations": {
          "orders_destination": {
            "Address": "https://localhost:8003/"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        }
      },
      "payments_cluster": {
        "Destinations": {
          "payments_destination": {
            "Address": "https://localhost:8004/"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        }
      },
      "products_cluster": {
        "Destinations": {
          "products_destination": {
            "Address": "https://localhost:8005/"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        }
      },
      "reports_cluster": {
        "Destinations": {
          "reports_destination": {
            "Address": "https://localhost:8006/"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        }
      }
    }
  }
}
